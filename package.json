{
  "name": "turborepo_next_nest",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install",
    "pre-commit": "turbo lint-staged"
  },
  "devDependencies": {
    "husky": "^8.0.0",
    "lint-staged": "^15.2.11",
    "prettier": "^3.2.5",
    "turbo": "^2.3.3",
    "typescript": "5.5.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.9.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "jose": "^5.9.6",
    "zod": "^3.24.1"
  }
}
